c43902efc22","children":["22ad68ba-2b41-4d56-9d99-c51db451d591"],"scripts":[{"name":"default","src":"this.value = \"\";\n\n// Attach this to the absolute position of the PARENT.\nthis.parentClip.getAbsPos = () => {\n    return this.paper.view.projectToView(new this.paper.Point(\n        this.parentClip.view.group.bounds.left,\n        this.parentClip.view.group.bounds.top\n    ));\n}\n\nthis.getAbsSize = () => {\n    return {\n        width: this.view.group.bounds.width * this.paper.view.zoom,\n        height: this.view.group.bounds.height * this.paper.view.zoom,\n    }\n}\n\nthis.updateDivPosition = () => {\n    var pos = this.parentClip.getAbsPos(); // Connect to parentClip's position.\n    var size = this.getAbsSize();\n    this.div.style.position = 'absolute';\n    this.div.style.left = pos.x + 'px';\n    this.div.style.top = pos.y + 'px';\n    this.div.style.width = size.width + 'px';\n    this.div.style.height = size.height + 'px';\n    this.div.style.backgroundColor = 'white';\n}\n\nthis.paper = this.view.paper;\nthis.div = document.createElement('div');\nthis.canvasContainer = this.project.view._canvasContainer;\nthis.canvasContainer.appendChild(this.div);\n\n"},{"name":"load","src":"/* TextInput code */\nthis._inputFunction = (val) => {\n    \n}\n\n// Adds an on change event to the text input. This function will be sent the text string\n// within the input.\nthis.setOnChange = (fn) => {\n    this._inputFunction = fn;\n}\n\nthis.parentClip.setOnChange = this.setOnChange;\n\nthis.onInputChange = (value) => {\n    this.value = value;\n    this._inputFunction && this._inputFunction(value);\n}\n\n// Update styling of the div.\nthis.inputElem = document.createElement('input');\nthis.inputElem.style.width = '100%';\nthis.inputElem.style.height = '100%';\nthis.inputElem.oninput = (e) => {\n    this.onInputChange(e.target.value)\n}\nthis.div.appendChild(this.inputElem);"},{"name":"update","src":"this.updateDivPosition();\n\nif (this.textStyle && !this._styleUpdated) {\n    let style = this.textStyle._json[1];\n    if (style) {\n        this.inputElem.style.fontWeight = style.fontWeight + \"\";\n        this.inputElem.style.fontSize = style.fontSize + \"\";\n        this.inputElem.style.fontFamily = style.fontFamily;\n        this._styleUpdated = true;\n    }\n    \n}\n\n// Update the value of the parent, to distribute values to the rest of the project.\nthis.parentClip.value = this.value;"},{"name":"unload","src":"if(this.div.parentElement) {\n    this.canvasContainer.removeChild(this.div);\n}\nthis._styleUpdated = false;\nthis.parentClip.value = this.value;"}],"cursor":"default","transformation":{"x":2,"y":1,"scaleX":1,"scaleY":1,"rotation":0,"opacity":1},"animationType":"loop","singleFrameNumber":1},{"classname":"Timeline","identifier":null,"name":null,"uuid":"22ad68ba-2b41-4d56-9d99-c51db451d591","children":["93688dfc-ecc7-415b-afd1-c41b5ce1038b"],"playheadPosition":1,"activeLayerIndex":0},{"classname":"Layer","identifier":null,"name":"Layer","uuid":"93688dfc-ecc7-415b-afd1-c41b5ce1038b","children":["0757a005-5ed1-45c0-888c-dd2c7964ff12"],"locked":false,"hidden":false},{"classname":"Frame","identifier":null,"name":null,"uuid":"0757a005-5ed1-45c0-888c-dd2c7964ff12","children":["20107987-5ca3-44b8-82e0-5e39cde1f0eb"],"scripts":[{"name":"default","src":""}],"cursor":"default","start":1,"end":1,"sound":null,"soundVolume":1,"soundLoop":false,"originalLayerIndex":0},{"classname":"Path","identifier":null,"name":null,"uuid":"20107987-5ca3-44b8-82e0-5e39cde1f0eb","children":[],"json":["Path",{"applyMatrix":true,"segments":[[-119.49441,16.77246],[-119.49441,-16.77246],[119.49441,-16.77246],[119.49441,16.77246]],"closed":true,"fillColor":[1,1,1,1],"strokeColor":[0,0,0],"strokeCap":"round"}],"fontStyle":"normal","fontWeight":400}],"assets":[]}